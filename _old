getVisibleItems = () => {
  let visible = this.props.items;

  if(this.props.filter.terms) {
    visible = visible.filter((item) => {
      return item.label
        .toLowerCase()
        .includes(this.props.filter.terms.toLowerCase())
    })
  }

  if(this.props.sort.id) {
    switch(this.props.sort.id) {
      case 'name':
        visible = visible.sort((a,b) => {
          return a.label.toLowerCase()[0] > b.label.toLowerCase()[0]
        })
        break;
      case 'date':
        visible = visible.sort((a,b) => {
          return a.date < b.date
        })
        break;
    }

    console.log('id', this.props.sort.id, 'visible', visible)
  }
  return visible;
}
